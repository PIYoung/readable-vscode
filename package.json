{
  "name": "readable",
  "displayName": "Readable",
  "description": "Instantly generate comprehensize + readable comments with Ai.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "commentai.rightClickComment",
          "group": "1_modification"
        }
      ]
    },
    "keybindings": [
      {
        "command": "commentai.rightClickComment",
        "key": "ctrl+shift+f8",
        "mac": "cmd+shift+f8",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "commentai.helloWorld",
        "title": "Readable: Hello World"
      },
      {
        "command": "commentai.rightClickComment",
        "title": "Generate Comment"
      },
      {
        "command": "commentai.commentFile",
        "title": "Readable: Generate comments for entire file."
      },
      {
        "command": "commentai.enableCodeLens",
        "title": "Readable: Enable Comment Suggestions"
      },
      {
        "command": "commentai.disableCodeLens",
        "title": "Readable: Disable Comment Suggestions"
      },
      {
        "command": "commentai.generateSummaryComment",
        "title": "Readable: Generate Summary of Selected Code"
      },
      {
        "command": "commentai.login",
        "title": "Readable: Login"
      },
      {
        "command": "commentai.register",
        "title": "Readable: Register"
      },
      {
        "command": "commentai.statusBarClicked",
        "title": "Readable: Show Menu"
      },
      {
        "command": "commentai.resetPassword",
        "title": "Readable: Reset Password"
      }
    ],
    "configuration": {
      "title": "CodeComment",
      "properties": {
        "commentai.enableCodeLens": {
          "type": "boolean",
          "default": true,
          "description": "Enables or disables the small comment text you see on top of functions."
        },
        "commentai.showFeedback": {
          "type": "boolean",
          "default": true,
          "description": "Enables or disables the feedback prompt after every completion."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^0.8.2",
    "axios": "^0.21.4",
    "https": "^1.0.0",
    "isomorphic-fetch": "^3.0.0"
  }
}
